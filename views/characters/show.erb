<h1><%= result["name"] %></h1>

<h3>Cards Available</h3>
<% if !cards.empty? %>
    <% cards.each do |card|%>
      <a href="/cards/<%= card["id"] %>"><p>Edition: <%= card["card_edition"] %>/<%=card["quantity_released"] %></p></a> 
    <% end %>
<% else%>
    <p>No cards available right now</p>
<% end%>
